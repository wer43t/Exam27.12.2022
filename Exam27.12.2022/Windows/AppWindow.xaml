<Window x:Class="Exam27._12._2022.Windows.AppWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Exam27._12._2022.Windows"
        mc:Ignorable="d"
        Title="AppWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="50*"/>
            <ColumnDefinition Width="250*"/>
            <ColumnDefinition Width="50*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="64*"/>
            <RowDefinition Height="225*"/>
            <RowDefinition Height="64*"/>
        </Grid.RowDefinitions>
        <Frame Grid.Row="1" Grid.Column="1" x:Name="appFrame" NavigationUIVisibility="Hidden"/>
        <Button x:Name="btnGoBack" Content="Назад" Margin="20" Click="btnGoBack_Click"/>
    </Grid>
</Window>
